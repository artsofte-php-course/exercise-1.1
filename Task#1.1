<?php
 
 
 
 function GetPaymentSystem($cardNumber){
 	if ($cardNumber[0] == '4')
 		return 'VISA';
 	elseif ($cardNumber[0] == '5')
 		return 'Mastercard';
 	elseif (substr($cardNumber, 0, 3) == '220')
 		return 'MIR';
 	else
 		return 'UNKNOWN';
 }
//$cardName = readline();

$cardNumber = '4561 2612 1234 5467';

$cardNumber = str_replace(['-', ' '], '',$cardNumber);
$oddSum = 0;
$evenSum = 0;
for ($i = 0; $i< strlen($cardNumber); $i++){
	if ($i%2==0){
		$oddNumber = (int)$cardNumber[$i]*2;
		$oddSum += $oddNumber<10 ? $oddNumber : $oddNumber - 9;
	}
	else{
		$evenSum += (int)$cardNumber[$i];		
	}
}
echo($oddSum+$evenSum % 10);
if (($oddSum+$evenSum) % 10 === 0)
	print_r(['isValid' => 'True', 'paymentSystem' => GetPaymentSystem($cardNumber)]);
else
	print_r(['isValid' => 'False', 'paymentSystem' => "UNKNOWN"]);


?>

